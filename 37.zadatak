#include <stdio.h>
/*
Omogucite korisniku unos BROJA te svaki od tih BROJA zbrojite s vrijednoscu od 3 do 29 te napravite tablicu.
*/
int main()
{
    int k;
    int BROJ;

    printf("Unesite BROJ: ");
    scanf_s("%d", &BROJ);

    for (k = 3; k <= 29; k++)
    {

        printf("%d + %d = %d\n", BROJ, k, (BROJ + k));
    }

    return 0;
}
/*
Korisnik je unio brojeve od 3 do 29 te svaki koji je unio zbrojio sa brojem čija je vrijednost od 3 do 29 i napravio tablicu.
*/ 
