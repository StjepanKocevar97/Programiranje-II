#include <stdio.h>
/*
Isprintajte sve vrijednosti slovenske abecede na ekran.
*/

int main()
{
    int s;

    for (s = 0; s <= 25; s++)
    {
        printf("Sve vrijednosti slovenske abecede  %c = %d\n", s, s);
    }

    return 0;
}
