#include <stdio.h>

/*Omogucite korisniku unos realnog broja te ispisite unatrag sve brojeve od unesenog broja pa da nule.*/

int main()
{
    int k;
    int broj;


    printf("Unesite neki broj: ");
    scanf("%f", &broj);

    for (k = broj; k >= 1; k--)
    {
        printf("%f\n", k);
    }
    return 0;
}

