#include <stdio.h>

/*Omogucite korisniku unos BROJA te ispisite unatrag sve brojeve od unesenog broja pa da nule.*/

int main()
{
    int k;
    int BROJ;


    printf("Unesite neki broj: ");
    scanf_s("%d", &BROJ);

    for (k = BROJ; k >= 1; k--)
    {
        printf("%d\n", k);
    }
    return 0;
}

